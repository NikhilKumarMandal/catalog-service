{
  "name": "catalog-service",
  "version": "1.0.0",
  "description": "a backend application where user can sell food online",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon src/server.ts",
    "build": "tsc",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "test": "jest --no-cache --coverage --all --runInBand",
    "test:watch": "jest --watch --runInBand",
    "start": "ts-node src/server.js"
  },
  "keywords": [
    "backend"
  ],
  "author": "nikhil kumar",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.8",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
  "dependencies": {
    "config": "^3.3.12",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "express-validator": "^7.2.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jwks-rsa": "^3.1.0",
    "mongoose": "^8.6.3",
    "morgan": "^1.10.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.1"
  }
}
